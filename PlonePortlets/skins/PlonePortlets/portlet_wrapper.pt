<div class="portlet" tal:attributes="id string:portlet-${here/portletName}">
        <h5>
            <span style="float:right">
               <img src="treeExpanded.gif" height="9" width="9" alt="expand/collapse" onclick="togglePortlet(this)" />
            </span>
            <span tal:content="here/Title">PORTLET TITLE</span>
        </h5>
        <div on-error="string:there was an error displaying this portlet" 
             class="portletBody" 
             tal:define="emitter here/getPortletEmitter">
             
           <div tal:content="structure python:getattr(here,emitter)()">
              Some dummy content
           </div>
           
                      
        </div>
        <div style="display:none" class="portlet-collapsed-info portletBody">&nbsp;</div>
</div>



