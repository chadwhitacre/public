#!/bin/sh
# 1) change the variables below as appropriate
# 2) cp this file somewhere on your path (e.g., /usr/local/bin)
# 3) chmod 755 /usr/local/bin/porter
# 4) sudo porter

export PYTHON=python
export PKG_HOME=/usr/local/lib/python2.4/site-packages/Porter
export INSTANCE_HOME=/usr/local/porter

$PYTHON $PKG_HOME/start.py