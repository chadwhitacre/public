<h1 tal:replace="structure context/manage_page_header">Header</h1>

<h2 tal:define="form_title string:Add an FCKeditor"
    tal:replace="structure here/manage_form_title">Form Title</h2>

<p class="form-help"> FCKeditor is a rich-text, WYSIWYG DHTML form widget. This
object manages and support multiple FCKeditor objects w/in the CMF.</p>

<form action="manage_addCMFFCKmanager"
      method="post"
      enctype="multipart/form-data">

<table cellspacing="0" cellpadding="2" border="0">
    <tr>
        <td align="left" valign="top">
            <div class="form-label">Id</div>
        </td>
        <td align="left" valign="top">
            <input type="text" name="id" size="40" />
        </td>
    </tr>
    <tr>
        <td align="left" valign="top">&nbsp;</td>
        <td align="left" valign="top">
            <div class="form-element">
                <input class="form-element"
                       type="submit"
                       name="submit"
                       value=" Add an FCKmanager for CMF " />
            </div>
        </td>
    </tr>
</table>
</form>

